package com.subject;

public class SubjectV{
	private int no;
	private String subjectNumber;
	private String subjectName;
	
	public SubjectVO() {
		
	}
	
	public SubjectVO(int no, String subjectNumber, String subjectName) {
		super();
		this.no = no;
		this.subjectName = subjectName;
		this.subjectNumber = subjectNumber;
	}
}